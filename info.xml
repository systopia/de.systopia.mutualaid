<?xml version="1.0"?>
<extension key="de.systopia.mutualaid" type="module">
  <file>mutualaid</file>
  <name>Mutual Aid Coordination</name>
  <description>Fight Corona by organising mutual aid in your neighbourhood with this CiviCRM Extension!</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>B. Endres / J. Schuppe</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.mutualaid</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.mutualaid</url>
    <url desc="Support">https://github.com/systopia/de.systopia.mutualaid/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate/>
  <version>1.2-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <requires>
    <ext>de.systopia.xcm</ext>
  </requires>
  <civix>
    <namespace>CRM/Mutualaid</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <upgrader>CRM_Mutualaid_Upgrader</upgrader>
</extension>
